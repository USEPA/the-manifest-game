{
  "tabWidth": 2,
  "plugins": ["prettier-plugin-tailwindcss"],
  "useTabs": false,
  "printWidth": 100,
  "singleQuote": true,
  "jsxSingleQuote": false,
  "endOfLine": "auto",
  "semi": true,
  "bracketSpacing": true,
  "bracketSameLine": false,
  "trailingComma": "es5",
  "overrides": [
    {
      "files": ["*.yaml", "*.yml"],
      "options": {
        "singleQuote": true
      }
    }
  ]
}
